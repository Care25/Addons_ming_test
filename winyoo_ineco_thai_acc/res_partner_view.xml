<?xml version="1.0"?>
<openerp>
    <data>
        
        <record model="ir.ui.view" id="view_winyoo_ineco_billing_partner_form">
	      	<field name="name">winyoo.ineco.billing.partner.form</field>
	      	<field name="model">res.partner</field>
	      	<field name="inherit_id" ref="ineco_thai_account.view_ineco_billing_partner_form"/>
	      	<field name="arch" type="xml">
					<field name="pid" position="replace"> 
                    	<field name="pid"
                    			string="Tax ID (เลข 13 หลัก)" 
                                attrs="{'required':[('is_company','=',True),('customer','=',True)]}"/> 
                    </field>
                    <field name="tax_detail" position="replace">                    
                    	<field name="tax_detail"
                    		string="Branch NO(เลขสาขา 5 ตัว)"
                    		placeholder="5 หลัก เช่น 00000 สำหรับสำนักงานใหญ่" 
                    		attrs="{'required':[('is_company','=',True),('customer','=',True)]}" />
                    </field>
                    <field name="with_holding_type" position="replace">
                    	<field name="with_holding_type" 
                    		string="Withholding tax(ภพ53/ภพ3)" 
                    		attrs="{'required':[('is_company','=',True),('customer','=',True)]}"/>
                    </field>

			</field>
		</record>        	                  
    </data>
</openerp>